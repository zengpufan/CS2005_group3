put>\r\n        </el-form-item>\r\n        <el-form-item prop=\"passwordConfirm\">\r\n            <el-input type=\"password\" v-model=\"registerForm.passwordConfirm\"\r\n                      auto-complete=\"off\" placeholder=\"确认密码\"></el-input>\r\n        </el-form-item>\r\n\r\n\r\n        <el-form-item style=\"width: 100%\">\r\n            <el-button type=\"primary\" style=\"width: 100%;background: #505458;border: none;\" v-on:click=\"register\">\r\n                立即注册\r\n            </el-button>\r\n        </el-form-item>\r\n        <el-form-item style=\"width: 100%\">\r\n            <el-button type=\"primary\" style=\"width: 100%;background: #505458;border: none;\" v-on:click=\"tologin\">返回\r\n            </el-button>\r\n        </el-form-item>\r\n    </el-form>\r\n    </body>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n\r\n    // eslint-disable-next-line vue/multi-word-component-names\r\n    name: 'Register',\r\n    data() {\r\n        var checkMobile = (rule, value, callback) => {\r\n            const regUser = /^((0\\d{2,3}-\\d{7,8})|(1[34578]\\d{9}))$/\r\n            if (regUser.test(value)) {\r\n                return callback()\r\n            }\r\n            callback(new Error('手机号码不能为空'))\r\n        }\r\n        var checkEmail = (rule, value, callback) => {\r\n            const regUser = /^([a-zA-Z0-9]+[-_]?)+@[a-zA-Z0-9]+\\.[a-z]+$/\r\n            if (regUser.test(value)) {\r\n                return callback()\r\n            }\r\n            callback(new Error('邮箱不能为空'))\r\n        }\r\n        return {\r\n            rules: {\r\n                username: [{required: true, message: '用户名不能为空', trigger: 'blur'}],\r\n                mobile: [{validate: checkMobile, trigger: 'blur'}],\r\n                email: [{validate: checkEmail, trigger: 'blur'}],\r\n                password: [{required: true, message: '密码不能为空', trigger: 'blur'}],\r\n                passwordConfirm: [{required: true, message: '密码不能为空', trigger: 'blur'}]\r\n            },\r\n            checked: true,\r\n            registerForm: {\r\n                username: '',\r\n                mobile: '',\r\n                email: '',\r\n                password: '',\r\n                passwordConfirm: ''\r\n            },\r\n            loading: false\r\n        }\r\n    },\r\n    methods: {\r\n        register() {\r\n            var _this = this\r\n            if (this.registerForm.password !== this.registerForm.passwordConfirm) {\r\n                this.$message({\r\n                    message: '两次输入的密码不一致',\r\n                    type: 'error'\r\n                })\r\n                return\r\n            }\r\n            this.$axios.post(\"/register\", {\r\n                username: this.registerForm.username,\r\n                password: this.registerForm.password,\r\n            })\r\n                .then(function (response) {\r\n                    // console.log(response.data.status)\r\n                    if (response.data.status === 200) {\r\n                        alert(\"注册成功\")\r\n                        //   _this.$message({\r\n                        //   message: '恭喜你，注册成功',\r\n                        //   type: 'success'\r\n                        // })\r\n                        _this.$router.replace('/login')\r\n                    } else {\r\n                        _this.$message({\r\n                            message: '该用户名已存在，请更换一个',\r\n                            type: 'error'\r\n                        })\r\n                    }\r\n                })\r\n                .catch(function (error) {\r\n                    console.log(error)\r\n                })\r\n        },\r\n        tologin() {\r\n            this.$router.replace('/login')\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n#paper {\r\n    background: url(\"../assets/img_1.png\") no-repeat center;\r\n    height: 80%;\r\n    width: 100%;\r\n    background-size: cover;\r\n    position: fixed;\r\n}\r\n\r\nbody {\r\n    margin: -5px 0px;\r\n}\r\n\r\n.register-container {\r\n    border-radius: 15px;\r\n    align-content: center;\r\n    background-clip: padding-box;\r\n    margin: 160px auto;\r\n    width: 350px;\r\n    padding: 35px 35px 15px 35px;\r\n    background: #fff;\r\n    border: 1px solid #eaeaea;\r\n    box-shadow: 0 0 25px #cac6c6;\r\n}\r\n\r\n.login_title {\r\n    margin: 0px auto 40px auto;\r\n    text-align: center;\r\n    color: #505458;\r\n}\r\n\r\n.login_remember {\r\n    margin: 0px 0px 35px 0px;\r\n    text-align: left;\r\n}\r\n</style>\r\n"],"mappings":"AAyCA;EAEA;EACAA,IAAA;EACAC,KAAA;IACA,IAAAC,WAAA,GAAAA,CAAAC,IAAA,EAAAC,KAAA,EAAAC,QAAA;MACA,MAAAC,OAAA;MACA,IAAAA,OAAA,CAAAC,IAAA,CAAAH,KAAA;QACA,OAAAC,QAAA;MACA;MACAA,QAAA,KAAAG,KAAA;IACA;IACA,IAAAC,UAAA,GAAAA,CAAAN,IAAA,EAAAC,KAAA,EAAAC,QAAA;MACA,MAAAC,OAAA;MACA,IAAAA,OAAA,CAAAC,IAAA,CAAAH,KAAA;QACA,OAAAC,QAAA;MACA;MACAA,QAAA,KAAAG,KAAA;IACA;IACA;MACAE,KAAA;QACAC,QAAA;UAAAC,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAC,MAAA;UAAAC,QAAA,EAAAd,WAAA;UAAAY,OAAA;QAAA;QACAG,KAAA;UAAAD,QAAA,EAAAP,UAAA;UAAAK,OAAA;QAAA;QACAI,QAAA;UAAAN,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAK,eAAA;UAAAP,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MACA;MACAM,OAAA;MACAC,YAAA;QACAV,QAAA;QACAI,MAAA;QACAE,KAAA;QACAC,QAAA;QACAC,eAAA;MACA;MACAG,OAAA;IACA;EACA;EACAC,OAAA;IACAC,SAAA;MACA,IAAAC,KAAA;MACA,SAAAJ,YAAA,CAAAH,QAAA,UAAAG,YAAA,CAAAF,eAAA;QACA,KAAAO,QAAA;UACAb,OAAA;UACAc,IAAA;QACA;QACA;MACA;MACA,KAAAC,MAAA,CAAAC,IAAA;QACAlB,QAAA,OAAAU,YAAA,CAAAV,QAAA;QACAO,QAAA,OAAAG,YAAA,CAAAH;MACA,GACAY,IAAA,WAAAC,QAAA;QACA;QACA,IAAAA,QAAA,CAAA9B,IAAA,CAAA+B,MAAA;UACAC,KAAA;UACA;UACA;UACA;UACA;UACAR,KAAA,CAAAS,OAAA,CAAAC,OAAA;QACA;UACAV,KAAA,CAAAC,QAAA;YACAb,OAAA;YACAc,IAAA;UACA;QACA;MACA,GACAS,KAAA,WAAAC,KAAA;QACAC,OAAA,CAAAC,GAAA,CAAAF,KAAA;MACA;IACA;IACAG,QAAA;MACA,KAAAN,OAAA,CAAAC,OAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}